{
  "extends": "airbnb",
  "rules": {
    "jsx-quotes": [2, "prefer-single"],
    "react/jsx-curly-spacing": [2, "always"],
    "react/prefer-stateless-function": [0],
    "react/jsx-filename-extension": [0],
    "react/forbid-prop-types": [0],
    "import/no-absolute-path": [0]
  },
  "settings": {
    "import/resolver": "webpack"
  }  
}
